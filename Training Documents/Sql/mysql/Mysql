select* from Employee;
create table Rasathi(EmpId integer not null primary key,EmpName varchar(20),salary float,designation varchar(20));
insert into Rasathi(EmpId,EmpName,salary,designation) values ('1373','Rasathi','13500','developer');
alter table Rasathi add column marital_status varchar(5) Default 'no';
insert into Rasathi(EmpId,EmpName,salary,designation) values ('1374','Raji','13500','developer');
insert into Rasathi(EmpId,EmpName,salary,designation,marital_status) values ('1375','Shami','13500','developer','yes');
alter table Rasathi rename column salary to Empsalary;
alter table Rasathi add column place varchar(5);
alter table Rasathi add column Fresher_or_Junior varchar(10) default 'fresher';
alter table Rasathi add column place varchar(10) default 'sivakasi';
alter table Rasathi rename column salary to Empsalary;
insert into Rasathi(EmpId,EmpName,Empsalary,designation,marital_status) values ('1376','priya','13500','developer','yes');
insert into Rasathi(EmpId,EmpName,Empsalary,designation,marital_status) values ('1377','priya Lakshmi','13500','developer','yes');
update Rasathi set marital_status='no' where empname='Shami';
select * from Rasathi order by empid;
alter table Rasathi rename fresher_or_junior to fresher_or_senior;
delete from Rasathi where Empname='priya';
alter table Rasathi rename to Employee;
update Employee set fresher_or_senior='senior' where empname='Shami';
select * from Employee order by empid;
select * into Rasathi from Employee; 
select * from Rasathi;
select * from Employee;
truncate table Rasathi;
